<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.mall.index.mapper.IndexMapper">
    <select id="getCustomerOrderListByCustomerId" parameterType="String"
            resultType="com.neusoft.mall.entity.OrderInfo">
        SELECT * FROM t_order WHERE customer_id = #{customerId}
    </select>
</mapper>