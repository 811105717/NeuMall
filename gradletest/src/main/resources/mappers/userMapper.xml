<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.stt.gradletest.user.dao.UserDao">

    <select id="getUserList" resultType="java.util.Map">
        select * from t_user
    </select>

    <select id="getUserListById" parameterType="java.util.HashMap" resultType="java.util.Map">
        select * from t_user where user_name = #{name} and user_id=#{id}
    </select>

    <select id="getUserListByModel" parameterType="com.stt.gradletest.user.model.UserVo" resultType="com.stt.gradletest.user.model.UserVo">
        select * from t_user
    </select>
</mapper>